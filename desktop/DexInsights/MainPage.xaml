<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DexInsights.MainPage">

    <ContentPage.Content>
        <Frame
       HasShadow="False"
       CornerRadius="0"
       HeightRequest="920"
       WidthRequest="1540">
            <Frame.Background>
                <LinearGradientBrush EndPoint="1,0">
                    <GradientStop Color="#A0FF00"
                          Offset="0.1" />
                    <GradientStop Color="Green"
                          Offset="1.0" />
                </LinearGradientBrush>
            </Frame.Background>
        </Frame>
    </ContentPage.Content>

</ContentPage>
